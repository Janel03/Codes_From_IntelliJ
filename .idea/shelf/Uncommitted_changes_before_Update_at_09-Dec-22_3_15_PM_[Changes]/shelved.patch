+
+    public Student(String firstName, String lastName, String rollNumber) {
+        this.firstName = firstName;
+        this.lastName = lastName;
+        this.rollNumber = rollNumber;
+    }
+    void printName(){
+        System.out.println(firstName+" "+lastName);
