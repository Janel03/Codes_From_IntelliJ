+            if (openTags.contains(split)) {
+                open.add(split);
+            }
+            if (closeTags.contains(split) && open.size() != 0) {
+                if (open.getLast().equals(split.substring(1))) open.removeLast();
+                else {
+                    return open.getLast();
+                }}}
+        return "true";
+    }}
